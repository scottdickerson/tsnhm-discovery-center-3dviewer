{
    "headers": [
        {
            "source": "**/*.@(jpg|jpeg|gif|png|obj|mtl)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "404.html",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=300"
                }
            ]
        }
    ]
}
