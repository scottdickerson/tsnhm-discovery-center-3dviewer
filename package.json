{
  "name": "tsnhm-discovery-center-3dviewer",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:body_fossils": "cross-env PUBLIC_INTERACTIVE='body_fossils' astro dev",
    "dev:tracks_and_traces": "cross-env PUBLIC_INTERACTIVE='tracks_and_traces' astro dev",
    "dev:putting_it_together": "cross-env PUBLIC_INTERACTIVE='putting_it_together' astro dev",
    "start": "astro dev",
    "build": "astro check && astro build && cp serve.json dist/",
    "build:body_fossils": "astro check && cross-env PUBLIC_INTERACTIVE='body_fossils' astro build && cp serve.json dist/",
    "build:tracks_and_traces": "astro check && cross-env PUBLIC_INTERACTIVE='tracks_and_traces' astro build && cp serve.json dist/",
    "build:putting_it_together": "astro check && cross-env PUBLIC_INTERACTIVE='putting_it_together' astro build && cp serve.json dist/",
    "preview": "astro preview",
    "astro": "astro",
    "format:fix": "prettier --write .",
    "production": "serve -l 3000 dist"
  },
  "dependencies": {
    "@astrojs/react": "^4.1.0",
    "@astrojs/tailwind": "^5.1.3",
    "@astrojs/vercel": "^8.0.0",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-visually-hidden": "^1.1.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^5.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html-react-parser": "^5.2.2",
    "lucide-react": "^0.474.0",
    "radix-ui": "^1.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.173.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@types/three": "^0.173.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.16"
  }
}
